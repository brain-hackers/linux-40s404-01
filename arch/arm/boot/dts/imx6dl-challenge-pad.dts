/dts-v1/;

#include "imx6dl.dtsi"

#include <dt-bindings/clock/imx6qdl-clock.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/sound/fsl-imx-audmux.h>

/ {
	model = "Benesse Challenge Pad";
	compatible = "benesse,challenge-pad", "fsl,imx6dl";

	chosen {
		stdout-path = &uart1;
	};

	memory@10000000 {
		device_type = "memory";
		reg = <0x10000000 0x40000000>;
	};

	reg_usb_otg_vbus: regulator-usb-otg-vbus {
		compatible = "regulator-fixed";
		regulator-name = "usb_otg_vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&gpio3 22 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		vin-supply = <&swbst_reg>;
	};

	reg_usb_h1_vbus: regulator-usb-h1-vbus {
		compatible = "regulator-fixed";
		regulator-name = "usb_h1_vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&gpio1 29 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		vin-supply = <&swbst_reg>;
	};

	reg_aux_5v: regulator-aux-5v {
		compatible = "regulator-fixed";
		regulator-name = "aux_5v";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&gpio6 10 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	reg_lcd: regulator-lcd-backlight {
		compatible = "regulator-fixed";
		regulator-name = "lcd";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio6 9 GPIO_ACTIVE_LOW>;
		enable-active-low;
	};

	reg_audio: regulator-audio {
		compatible = "regulator-fixed";
		regulator-name = "wm8962-supply";
		gpio = <&gpio4 10 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-always-on;
	};

	reg_audio_amp: regulator-audio-amp {
		compatible = "regulator-fixed";
		regulator-name = "wm8962-supply-amp";
		gpio = <&gpio6 7 GPIO_ACTIVE_LOW>;
		enable-active-low;
		regulator-always-on;
	};

	reg_audio_clock: regulator-audio-clock {
		compatible = "regulator-fixed";
		regulator-name = "wm8962-supply-clock";
		gpio = <&gpio6 16 GPIO_ACTIVE_LOW>;
		enable-active-low;
		regulator-always-on;
	};

	reg_touch: regulator-touchscreen {
		compatible = "regulator-fixed";
		regulator-name = "bu21025-supply";
		gpio = <&gpio3 24 GPIO_ACTIVE_LOW>;
		enable-active-low;
	};

	reg_camera: regulator-sr200pc20 {
		compatible = "regulator-fixed";
		regulator-name = "sr200pc20-supply";
		gpio = <&gpio6 8 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	reg_wifi: regulator-sd8787 {
		compatible = "regulator-fixed";
		regulator-name = "sd8787-supply";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio6 15 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

    reg_wifi_reset: regulator-sd8787-reset {
		compatible = "regulator-fixed";
		regulator-name = "sd8787-supply-reset";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio6 14 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_keys>;

		power {
			label = "Power Button";
			gpios = <&gpio3 25 GPIO_ACTIVE_LOW>;
			wakeup-source;
			linux,code = <KEY_POWER>;
		};

		volume-up {
			label = "Volume Up";
			gpios = <&gpio1 4 GPIO_ACTIVE_LOW>;
			wakeup-source;
			linux,code = <KEY_VOLUMEUP>;
		};

		volume-down {
			label = "Volume Down";
			gpios = <&gpio1 5 GPIO_ACTIVE_LOW>;
			wakeup-source;
			linux,code = <KEY_VOLUMEDOWN>;
		};

		home {
			label = "Home Button";
			gpios = <&gpio2 4 GPIO_ACTIVE_LOW>;
			wakeup-source;
			linux,code = <KEY_HOME>;
		};
	};

	sound {
		compatible = "simple-audio-card";
		simple-audio-card,name = "Wolfson WM8978";
		simple-audio-card,format = "i2s";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_hp>;
		simple-audio-card,widgets =
            "Microphone", "MIC",
			"Microphone", "Mic Jack",
            "Headphone", "Headphones",
            "Speaker", "Ext Spk";
		simple-audio-card,routing =
			"Headphones", "LHP",
			"Headphones", "RHP",
			"Ext Spk", "LSPK",
			"Ext Spk", "RSPK",
			"RMICN", "MIC",
            "RMICP", "MIC",
            "MIC", "Mic Bias",
			"LMICN", "Mic Jack",
            "LMICP", "Mic Jack",
            "Mic Jack", "Mic Bias";
		simple-audio-card,hp-det-gpio = <&gpio7 8 GPIO_ACTIVE_HIGH>;
		simple-audio-card,mic-det-gpio = <&gpio1 9 GPIO_ACTIVE_HIGH>;
		simple-audio-card,bitclock-master = <&codec_dai>;
		simple-audio-card,frame-master = <&codec_dai>;
		cpu_dai: simple-audio-card,cpu {
            sound-dai = <&ssi2>;
        };
        codec_dai: simple-audio-card,codec {
            sound-dai = <&codec>;
			clocks = <&clks IMX6QDL_CLK_CKO>;
			system-clock-frequency=<24000000>;
        };
	};

	backlight_lvds: backlight-lvds {
		compatible = "pwm-backlight";
		pwms = <&pwm1 0 50000>;
		power-supply = <&reg_lcd>;
		brightness-levels = <0 4 8 16 32 64 128 255>;
		default-brightness-level = <7>;
		status = "okay";
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_leds>;

		red {
			gpios = <&gpio1 2 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		flashing_off {
			gpios = <&gpio2 2 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		osirase_c {
			gpios = <&gpio2 3 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};

	panel {
		compatible = "simple-panel";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_panel>;

		power-supply = <&reg_aux_5v>;
		enable-gpios = <&gpio1 30 GPIO_ACTIVE_HIGH>;
		backlight = <&backlight_lvds>;
	
		port {
			panel_in: endpoint {
				remote-endpoint = <&lvds0_out>;
			};
		};
	};
};

&ipu1_csi0_from_ipu1_csi0_mux {
	bus-width = <8>;
	data-shift = <12>; /* Lines 19:12 used */
	hsync-active = <1>;
	vsync-active = <1>;
};

&ipu1_csi0 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ipu1_csi0>;
};

&mipi_csi {
	status = "okay";

	port@0 {
		reg = <0>;

		mipi_csi0_in: endpoint {
			remote-endpoint = <&sr200pc20_to_mipi_csi0>;
			clock-lanes = <0>;
			data-lanes = <1 2>;
		};
	};
};

&audmux {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_audmux>;
	status = "okay";

	port2 {
        fsl,audmux-port = <1>;
        fsl,port-config = <
				IMX_AUDMUX_V2_PTCR_SYN			0
                IMX_AUDMUX_V2_PTCR_TFSEL(2)		0
                IMX_AUDMUX_V2_PTCR_TCSEL(2)		0
                IMX_AUDMUX_V2_PTCR_TFSDIR		0
                IMX_AUDMUX_V2_PTCR_TCLKDIR      IMX_AUDMUX_V2_PDCR_RXDSEL(2)>;
    };
    port3 {
        fsl,audmux-port = <2>;
        fsl,port-config = <
				IMX_AUDMUX_V2_PTCR_SYN          IMX_AUDMUX_V2_PDCR_RXDSEL(1)>;
    };
};

&clks {
	assigned-clocks = <&clks IMX6QDL_CLK_LDB_DI0_SEL>,
			  <&clks IMX6QDL_CLK_LDB_DI1_SEL>;
	assigned-clock-parents = <&clks IMX6QDL_CLK_PLL3_USB_OTG>,
				 <&clks IMX6QDL_CLK_PLL3_USB_OTG>;
};

&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";

	codec: wm8978@1a {
		compatible = "wlf,wm8978";
		reg = <0x1a>;
		#sound-dai-cells = <0>;
	};

	sr200pc20: camera@20 {
		compatible = "siliconfile,sr200pc20";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_sr200pc20>;
		reg = <0x20>;
		clocks = <&clks IMX6QDL_CLK_OSC>;
		clock-names = "osc_clk";
		power-supply = <&reg_camera>;
		powerdown-gpios = <&gpio1 16 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio1 17 GPIO_ACTIVE_LOW>;

		port {
			sr200pc20_to_mipi_csi0: endpoint {
				remote-endpoint = <&mipi_csi0_in>;
				clock-lanes = <0>;
				data-lanes = <1 2>;
			};
		};
	};

	accelerometer@1c {
		compatible = "fsl,mma8452";
		reg = <0x1c>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c1_mma8451_int>;
		interrupt-parent = <&gpio1>;
		interrupts = <18 IRQ_TYPE_LEVEL_LOW>;
	};

	fuelgauge: max17048@36 {
		compatible = "maxim,max17048";
		reg = <0x36>;

		maxim,double-soc;
		maxim,rcomp = /bits/ 8 <0x4d>;

		interrupt-parent = <&gpio4>;
		interrupts = <5 IRQ_TYPE_LEVEL_LOW>;

		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c1_max17048_int>;

		maxim,alert-low-soc-level = <2>;
	};
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

	pmic: pfuze100@8 {
		compatible = "fsl,pfuze100";
		reg = <0x08>;

		regulators {
			sw1a_reg: sw1ab {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw1c_reg: sw1c {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw2_reg: sw2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw3a_reg: sw3a {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3b_reg: sw3b {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw4_reg: sw4 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			swbst_reg: swbst {
				regulator-min-microvolt = <5000000>;
				regulator-max-microvolt = <5150000>;
			};

			snvs_reg: vsnvs {
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <3000000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vref_reg: vrefddr {
				regulator-boot-on;
				regulator-always-on;
			};

			vgen1_reg: vgen1 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen2_reg: vgen2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen3_reg: vgen3 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
			};

			vgen4_reg: vgen4 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen5_reg: vgen5 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen6_reg: vgen6 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};
		};
	};

	bu21013_tp@48 {
		compatible = "rohm,bu21025gul";
		reg = <0x48>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c2_bu21025_int>;
		interrupt-parent = <&gpio7>;
		interrupts = <1 IRQ_TYPE_EDGE_FALLING>;
		irq-gpio = <&gpio7 1 0>;
		power-supply = <&reg_touch>;
	};
};

&iomuxc {
	pinctrl-names = "default";

	imx6dl-challenge-pad {

		pinctrl_gpio_panel: gpio_panelgrp {
			fsl,pins = <
				MX6QDL_PAD_ENET_TXD0__GPIO1_IO30	0x1b0b0 /* LCD_POWER_EN */
				MX6QDL_PAD_NANDF_WP_B__GPIO6_IO09	0x1b0b0 /* LCD_BACKLIGHT_POWER_EN */
				MX6QDL_PAD_NANDF_RB0__GPIO6_IO10	0x1b0b0 /* AUX_5V_ENABLE */
			>;
		};

		pinctrl_sr200pc20: gpio_sr200pc20 {
			fsl,pins = <
				MX6QDL_PAD_SD1_DAT0__GPIO1_IO16		0x1b0b0
				MX6QDL_PAD_SD1_DAT1__GPIO1_IO17		0x1b0b0
				MX6QDL_PAD_NANDF_ALE__GPIO6_IO08	0x1b0b0
			>;
		};

		pinctrl_audmux: audmuxgrp {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL2__GPIO4_IO10		0x1b0b0
				MX6QDL_PAD_NANDF_CS3__GPIO6_IO16	0x1b0b0
				MX6QDL_PAD_NANDF_CLE__GPIO6_IO07	0x1b0b0
				MX6QDL_PAD_GPIO_0__CCM_CLKO1		0x1b0b0

				MX6QDL_PAD_CSI0_DAT7__AUD3_RXD		0x130b0
				MX6QDL_PAD_CSI0_DAT4__AUD3_TXC		0x130b0
				MX6QDL_PAD_CSI0_DAT5__AUD3_TXD		0x110b0
				MX6QDL_PAD_CSI0_DAT6__AUD3_TXFS		0x130b0
			>;
		};

		pinctrl_gpio_keys: gpio_keysgrp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D25__GPIO3_IO25  	0x1b0b0 /* Power */
				MX6QDL_PAD_GPIO_4__GPIO1_IO04   	0x1b0b0 /* Vol Up */
				MX6QDL_PAD_GPIO_5__GPIO1_IO05		0x1b0b0 /* Vol Down */
				MX6QDL_PAD_NANDF_D4__GPIO2_IO04		0x1b0b0 /* Home */
			>;
		};

		pinctrl_hp: hpgrp {
			fsl,pins = <
				MX6QDL_PAD_SD3_RST__GPIO7_IO08          0x1b0b0
				MX6QDL_PAD_GPIO_9__GPIO1_IO09           0x1b0b0
			>;
		};

		pinctrl_i2c1: i2c1grp {
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT8__I2C1_SDA		0x4001b8b1
				MX6QDL_PAD_CSI0_DAT9__I2C1_SCL		0x4001b8b1
			>;
		};

		pinctrl_i2c1_mma8451_int: i2c1mma8451intgrp {
			fsl,pins = <
				MX6QDL_PAD_SD1_CMD__GPIO1_IO18		0xb0b1
			>;
		};

		pinctrl_i2c1_max17048_int: i2c1max17048intgrp {
			fsl,pins = <
				MX6QDL_PAD_GPIO_19__GPIO4_IO05		0xb0b1
			>;
		};

		pinctrl_i2c2: i2c2grp {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL3__I2C2_SCL		0x4001b8b1
				MX6QDL_PAD_KEY_ROW3__I2C2_SDA		0x4001b8b1
			>;
		};

		pinctrl_i2c2_bu21025_int: i2c2bu21025intgrp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D24__GPIO3_IO24	0x1b0b0
				MX6QDL_PAD_SD3_DAT4__GPIO7_IO01 0xb0b1
			>;
		};

		pinctrl_ipu1_csi0: ipu1csi0grp {
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT12__IPU1_CSI0_DATA12    0x1b0b0
				MX6QDL_PAD_CSI0_DAT13__IPU1_CSI0_DATA13    0x1b0b0
				MX6QDL_PAD_CSI0_DAT14__IPU1_CSI0_DATA14    0x1b0b0
				MX6QDL_PAD_CSI0_DAT15__IPU1_CSI0_DATA15    0x1b0b0
				MX6QDL_PAD_CSI0_DAT16__IPU1_CSI0_DATA16    0x1b0b0
				MX6QDL_PAD_CSI0_DAT17__IPU1_CSI0_DATA17    0x1b0b0
				MX6QDL_PAD_CSI0_DAT18__IPU1_CSI0_DATA18    0x1b0b0
				MX6QDL_PAD_CSI0_DAT19__IPU1_CSI0_DATA19    0x1b0b0
				MX6QDL_PAD_CSI0_PIXCLK__IPU1_CSI0_PIXCLK   0x1b0b0
				MX6QDL_PAD_CSI0_MCLK__IPU1_CSI0_HSYNC      0x1b0b0
				MX6QDL_PAD_CSI0_VSYNC__IPU1_CSI0_VSYNC     0x1b0b0
			>;
		};

		pinctrl_pwm1: pwm1grp {
			fsl,pins = <
				MX6QDL_PAD_SD1_DAT3__PWM1_OUT		0x17059
			>;
		};

		pinctrl_uart1: uart1grp {
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT10__UART1_TX_DATA	0x1b0b1
				MX6QDL_PAD_CSI0_DAT11__UART1_RX_DATA	0x1b0b1
			>;
		};

		pinctrl_uart2: uart2grp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D26__UART2_TX_DATA	0x1b0b1
				MX6QDL_PAD_EIM_D27__UART2_RX_DATA	0x1b0b1
				MX6QDL_PAD_EIM_D28__UART2_CTS_B		0x1b0b1
				MX6QDL_PAD_EIM_D29__UART2_RTS_B		0x1b0b1
			>;
		};

		pinctrl_bluetooth: bluetoothgrp {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL1__AUD5_TXFS	0x100b1
				MX6QDL_PAD_KEY_ROW0__AUD5_TXD	0x100b1
				MX6QDL_PAD_KEY_COL0__AUD5_TXC	0x100b1
				MX6QDL_PAD_KEY_ROW1__AUD5_RXD	0x1b0b0
			>;
		};

		pinctrl_usbotg: usbotggrp {
			fsl,pins = <
				MX6QDL_PAD_ENET_RX_ER__USB_OTG_ID	0x17059
			>;
		};

		pinctrl_usdhc2: usdhc2grp {
			fsl,pins = <
				MX6QDL_PAD_SD2_CMD__SD2_CMD		0x17059
				MX6QDL_PAD_SD2_CLK__SD2_CLK		0x10059
				MX6QDL_PAD_SD2_DAT0__SD2_DATA0		0x17059
				MX6QDL_PAD_SD2_DAT1__SD2_DATA1		0x17059
				MX6QDL_PAD_SD2_DAT2__SD2_DATA2		0x17059
				MX6QDL_PAD_SD2_DAT3__SD2_DATA3		0x17059

				MX6QDL_PAD_GPIO_8__XTALOSC_REF_CLK_32K	0x1b0b0
				MX6QDL_PAD_NANDF_CS0__GPIO6_IO11	0x1b0b0
				MX6QDL_PAD_NANDF_CS1__GPIO6_IO14	0x1b0b0
				MX6QDL_PAD_NANDF_CS2__GPIO6_IO15	0x1b0b0
			>;
		};

		pinctrl_usdhc3: usdhc3grp {
			fsl,pins = <
				MX6QDL_PAD_SD3_CMD__SD3_CMD		0x17059
				MX6QDL_PAD_SD3_CLK__SD3_CLK		0x10059
				MX6QDL_PAD_SD3_DAT0__SD3_DATA0		0x17059
				MX6QDL_PAD_SD3_DAT1__SD3_DATA1		0x17059
				MX6QDL_PAD_SD3_DAT2__SD3_DATA2		0x17059
				MX6QDL_PAD_SD3_DAT3__SD3_DATA3		0x17059
			>;
		};

		pinctrl_usdhc4: usdhc4grp {
			fsl,pins = <
				MX6QDL_PAD_SD4_CMD__SD4_CMD		0x17059
				MX6QDL_PAD_SD4_CLK__SD4_CLK		0x10059
				MX6QDL_PAD_SD4_DAT0__SD4_DATA0		0x17059
				MX6QDL_PAD_SD4_DAT1__SD4_DATA1		0x17059
				MX6QDL_PAD_SD4_DAT2__SD4_DATA2		0x17059
				MX6QDL_PAD_SD4_DAT3__SD4_DATA3		0x17059
				MX6QDL_PAD_SD4_DAT4__SD4_DATA4		0x17059
				MX6QDL_PAD_SD4_DAT5__SD4_DATA5		0x17059
				MX6QDL_PAD_SD4_DAT6__SD4_DATA6		0x17059
				MX6QDL_PAD_SD4_DAT7__SD4_DATA7		0x17059
			>;
		};

		pinctrl_wdog: wdoggrp {
			fsl,pins = <
				MX6QDL_PAD_GPIO_1__WDOG2_B		0x1b0b0
			>;
		};
	};

	gpio_leds {
		pinctrl_gpio_leds: gpioledsgrp {
			fsl,pins = <
				MX6QDL_PAD_GPIO_2__GPIO1_IO02   0x1b0b0 /* USR_DEF_RED_LED */
				MX6QDL_PAD_NANDF_D2__GPIO2_IO02 0x1b0b0 /* Flashing_off */
				MX6QDL_PAD_NANDF_D3__GPIO2_IO03 0x1b0b0 /* osirase_c */
			>;
		};
	};
};

&ldb {
	status = "okay";

	lvds-channel@1 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <24>;
		status = "okay";

		port@0 {
			reg = <0>;

			lvds0_out: endpoint {
				remote-endpoint = <&panel_in>;
			};
		};
		display-timings {
	        timing0 {
        	    clock-frequency = <15385000>;
            	hactive = <1024>;
	            vactive = <768>;
    	        hsync-len = <70>;
        	    hfront-porch = <160>;
            	hback-porch = <160>;
	            vfront-porch = <15>;
    	        vback-porch = <23>;
        	    vsync-len = <10>;
	        };
    	};
	};
};

&pwm1 {
	#pwm-cells = <2>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm1>;
	status = "okay";
};

&reg_arm {
       vin-supply = <&sw1a_reg>;
};

&reg_pu {
       vin-supply = <&sw1c_reg>;
};

&reg_soc {
       vin-supply = <&sw1c_reg>;
};

&reg_vdd1p1 {
	vin-supply = <&vgen5_reg>;
};

&reg_vdd2p5 {
	vin-supply = <&vgen5_reg>;
};

&snvs_poweroff {
	status = "okay";
};

&snvs_pwrkey {
	status = "okay";
};

&ssi2 {
	fsl,mode = "i2s-master";
	status = "okay";
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

&uart2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart2>;
	status = "okay";
};

&usbh1 {
	vbus-supply = <&reg_usb_h1_vbus>;
	dr_mode = "otg";
	status = "okay";
};

&usbotg {
	vbus-supply = <&reg_usb_otg_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg>;
	disable-over-current;
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2>;
	bus-width = <4>;
	cap-power-off-card;
	keep-power-in-suspend;
	vmmc-supply = <&reg_wifi>;
	vqmmc-supply = <&reg_wifi_reset>;
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;

	mwifiex: wifi@1 {
		compatible = "marvell,sd8787", "marvell,sd8897";
		reg = <0x1>;
		status = "okay";
	};

/*	btmrvl: bluetooth@2 {
		compatible = "marvell,sd8897-bt";
		reg = <0x2>;
		status = "okay";
	};*/
};

&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3>;
	bus-width = <4>;
	cd-gpios = <&gpio2 0 GPIO_ACTIVE_LOW>;
	wp-gpios = <&gpio2 1 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&usdhc4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc4>;
	bus-width = <8>;
	non-removable;
	no-1-8-v;
	status = "okay";
};

&wdog1 {
	status = "disabled";
};

&wdog2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_wdog>;
	fsl,ext-reset-output;
	status = "okay";
};
